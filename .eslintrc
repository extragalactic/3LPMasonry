{
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "no-tabs": 0,
    "indent": ["error", "tab"],
    "no-use-before-define": 0,
    "react/jsx-indent-props": [2, "tab"],    
    "react/jsx-indent": [2, "tab"],
    "no-underscore-dangle": 0,
    "max-len": 0
  },
  "parser": "babel-eslint"
}